<h1>Patients</h1>

<%= @patient.name %>
<% @patient.appointments.each do |appointment| %>
  <% @patient_doctor = @patient.doctors.find(appointment.doctor_id) %>
  <%= link_to @patient_doctor.name, doctor_path(@patient_doctor) %> - <%= appointment.appointment_datetime.strftime("%B %d, %Y at %H:%M") %>
<% end %> 